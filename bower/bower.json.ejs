{
  "name": "<%- pkg.name %>",
  "version": "<%- pkg.version %>",
  <% if (pkg.author) { %>
  "authors": [
    "<%- pkg.author %>"
  ]
  <% } %>
  <% if (pkg.license) { %>"license": "<%- pkg.license %>",<% } %>
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    <% var i = 1; %>
    <% for(var key in deps) { %>
    "<%- key %>": "<%- deps[key] %><% if (i < numDeps) { %>,<% } %>
    <% i++; %>
    <% } %>
  }
}
